<div class="item">
    <div>Email</div>
    <div class="input">
        <input  type="email" name="email" placeholder="Digite seu email" value="{{cordinator.email}}" onblur="Validate.apply(this, 'isEmail')">
    </div>
</div>

<div class="item">
    <div>Nova senha</div>
    <div class="input">
        <input type="password" name="password" placeholder="Digite nova senha">
    </div>
</div>

<div class="item">
    <div>Repita a nova senha</div>
    <div class="input">
        <input type="password" name="passwordRepeat" placeholder="Repita a senha">
    </div>
</div>

<input type="hidden" name="token" value="{{token}}">